linux:
	gcc -c -fPIC -O2 start.c
	gcc -c -fPIC -O2 print.c
	gcc -c -fPIC -O2 remember.c
	gcc -c -fPIC -O2 else.c
	gcc -c -fPIC -O2 identify.c
	gcc -c -fPIC -O2 read.c
cross:
	x86_64-elf-gcc -c -fPIC -O2 start.c
	x86_64-elf-gcc -c -fPIC -O2 print.c
	x86_64-elf-gcc -c -fPIC -O2 remember.c
	x86_64-elf-gcc -c -fPIC -O2 else.c
	x86_64-elf-gcc -c -fPIC -O2 identify.c
	x86_64-elf-gcc -c -fPIC -O2 read.c
