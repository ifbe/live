binary:
	gcc -c -fPIC -O2 circle.c
	gcc -c -fPIC -O2 math.c
crosscompile:
	x86_64-elf-gcc -c -fPIC -O2 circle.c
	x86_64-elf-gcc -c -fPIC -O2 math.c
linux:
	gcc -c -fPIC -O2 circle.c
	gcc -c -fPIC -O2 math.c
mac:
	clang -c -fPIC -O2 circle.c
	clang -c -fPIC -O2 math.c
win:
	gcc -c -O2 circle.c
	gcc -c -O2 math.c